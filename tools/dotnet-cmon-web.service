[Unit]
    Description=CMon.Web

    [Service]
    WorkingDirectory=/var/cmon.web
    ExecStart=/usr/bin/dotnet /var/cmon.web/CMon.Web.dll
    Restart=always
    RestartSec=10
    SyslogIdentifier=dotnet-cmon-web
    User=blackish
    Environment=ASPNETCORE_ENVIRONMENT=Production 

    [Install]
    WantedBy=multi-user.target

